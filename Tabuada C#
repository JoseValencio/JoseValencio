using System;

public class Tabuada
{
    public static void Main(string[] args)
    {
        int valor = 10; 
        
        Console.WriteLine("Informe o Numero da Tabuada");
        
        int N = Convert.ToInt32(Console.ReadLine());
        
        for (int i=0; i <= valor; i++)
        {
            int mult = i * N;
            
            Console.WriteLine(i + " x " + N + " = " + mult );
        }
    }
}
